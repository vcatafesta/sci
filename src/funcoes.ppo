function AchaSegunda( dDate )

    if cDow( dDate ) = "Monday"
        return( dDate + 7 )
    endif
    For i = 1 To 7
         if cDow( dDate + i ) = "Monday"
             return( dDate + i )
         endif
    Next
    return dDate

