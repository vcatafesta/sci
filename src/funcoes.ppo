function Instru80( nQualPorta )
    LOCAL cScreen                := SaveScreen()
    LOCAL Arq_Ant                := Alias()
    LOCAL Ind_Ant                := IndexOrd()
    LOCAL nChoice
    LOCAL aNewLpt
    LOCAL i                        := 0
    LOCAL nStatus                := 0
