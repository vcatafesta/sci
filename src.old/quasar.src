                  @ nlinha, 51 say cmarca
                  @ nlinha, 63 say ccodfor
                  @ nlinha, 86 say cunidade
                  @ nlinha, 93 say nprecovist picture "999,999.99"
               endif
               skip 
               nlinha++
            enddo
            if (ccabecalho != 0 .AND. ctiprel = "3")
               nlinha:= 99
            endif
            select Grupo
            skip 
         enddo
         if (ctiprel = "1")
