               endif
            endif
            @ nlinha,  1 say valfr
            @ nlinha, 10 say chistor
            @ nlinha, 42 say cdocto
            @ nlinha, 53 say chistorico
            @ nlinha, 66 say cdebcred picture "999,999,999.99"
