        EscolheImpressoraUsuario(nil, nil, nil)
    endif

    if nQualPorta <> NIL
        nQualPorta := nPortaDeImpressao
        return( .T. )
    endif
    ErrorBeep()
    while(.T.)
        oMenu:Limpa()
        aAction    := { "PRONTA         ","FORA DE LINHA  ","DESLIGADA      ","SEM PAPEL      ", "NAO CONECTADA  "}
        aComPort := { "DISPONIVEL     ","INDISPONIVEL   " }
