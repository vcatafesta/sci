   @ 23,  0 clear to 23, 70
   @ lin,  0 say ""
   do case
   case mode = 0
      if (Deleted())
         set color to (corlinmenu)
         @ 23, 30 say "DELETADO"
      else
         @ 23, 30 say Replicate(" ", 9)
      endif
      set color to (ccor1)
      return 1
   case mode = 1
