For Conta := Len( Numero ) To 1 Step -1
    Num := Val( SubStr( Numero, Conta, 1 ) )
    Mult++
    Soma += ( Num * Mult )
next
Dig := Mod( ( Soma * 10), 11 )
if Dig >= 10
    Dig := 0
endif
d1 := Dig
Numero = Left( Var2, 10 )
Mult     := 1
Soma     := 0
Dig     := 0
Prod     := 0
Num     := 0
For Conta := Len( Numero ) To 1 Step -1
    Num := Val( SubStr( Numero, Conta, 1 ) )
    Mult++
    Soma += ( Num * Mult )
next
Dig := Mod( ( Soma * 10), 11 )
if Dig >= 10
