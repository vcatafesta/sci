procedure MATRIZLINM(Arg1)

   close databases
   select 1
   close format
   abredbf("MENUS")
   set index to MENUS1
   filtrar1(4, Arg1)
   goto top
   i:= 1
   do while (!EOF())
      menu[1][i]:= Str(i, 2) + "-" + Trim(menus->menu)
      menu[2][i]:= menus->executa
      menu[3][i]:= menus->codigo
      skip 
      if (!EOF() .AND. Len(Trim(menus->codigo)) = 4)
         asize(menu[1], i + 1)
         asize(menu[2], i + 1)
         asize(menu[3], i + 1)
         i++
      endif
   enddo
   i:= 1
   ncol:= 2
   for i:= 1 to Len(menu[1])
      @  0,  0 say ""
      @  1, ncol prompt menu[1][i]
      ncol:= ncol + 15
   next
   close
   return

