            SaidaParaHtml()
            return( .T. )
        Case nChoice = 11
            nPortaDeImpressao := 1
            SaidaParaSpooler()
            return( .T. )
        OTherWise
            nPortaDeImpressao   := Iif( nChoice = 0, 1, nChoice )
            oAmbiente:cArquivo  := ""
            oAmbiente:Spooler   := .F.
            oAmbiente:PrintReady := nChoice
