function aMaxStrLen( xArray )

    LOCAL nTam    := Len(xArray)
    LOCAL nLen    := 0
    LOCAL nMaxLen := 0
    LOCAL x

    For x := 1 To nTam
        nLen := Len(xArray[x])
        if nMaxLen < nLen
            nMaxLen := nLen
        endif
    Next
    return( nMaxLen )

