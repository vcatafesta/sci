      rodape(nalturafor, .F., 134)
      ccabecalho++
      nlinha:= 7
      eject
      @  1,  1 say "" + cnomemp + ""
      @  1,  2 say ""
      @  2,  1 say "Relatorio de contas a receber  " + ccabcond + ;
         Replicate(" ", 70) + "Pagina :" + Str(ccabecalho, 3)
      @  3,  1 say impcontasa
      @  4,  1 say clinha
      @  5,  1 say impcontasa
   endif
   carea:= Select()
   select Clientes
   seek Arg1
   if (Found())
      @ nlinha,  1 say ;
         "Endereco                           Bairro         Cidade                      UF  Fone       CEP"
      nlinha++
