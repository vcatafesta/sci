function SAI(Arg1)

   if (Arg1 = "" .OR. Arg1 = Nil)
      Arg1:= "Deseja Abandonar Esta Rotina ?"
   endif
   if (LastKey() == K_ESC)
      nresp:= alert(Arg1, vet)
      if (nresp = 1)
         lcontrole:= .F.
         return .T.
      elseif (nresp = 2)
         lcontrole:= .T.
         return .T.
      elseif (LastKey() = K_ESC)
         lcontrole:= .F.
         return .T.
      endif
   endif
   lcontrole:= .T.
   return .F.

