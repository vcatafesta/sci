function DOWNMENU(Arg1, Arg2, Arg3, Arg4, Arg5)



    Local Local1, Local2, Local3, Local4, Local5, Local6, Local7,     Local8
    Local1:= htk:= 0
    Local2:= setcursor(0)
    Local3:= Len(Arg1)
    Local4:= Arg1[1][10]
