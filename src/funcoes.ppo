function nSetColor(std, enh, uns)




    LOCAL cStd,     cEnh,     cUns,     cColor

    cStd     := attrtoa(std)
    cEnh     := attrtoa(enh)
    cUns     := attrtoa(uns)


    ColorStandard(std)
    ColorEnhanced(enh)
    ColorUnselected(uns)
    cColor := cStd + "," + cEnh + ",,," + cUns




    Setcolor( cColor )
    return COlorStrToInt(cColor)

function cSetColor(ColorStr)



LOCAL nStd,     nEnh,     nUns

