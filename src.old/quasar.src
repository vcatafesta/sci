function GERTEMP361(Arg1, Arg2)

   local Local1
   Local1:= .F.
   select 2
   if (!abredbf("ENTRADAS", "ENTRADA3.NTX"))
      return .F.
   endif
   select 3
   if (!abredbf("FORNECED", "FORNECE1.NTX"))
      return .F.
   endif
   select 1
   use (dbcreate)
   select 2
   set softseek on
   seek Arg1
   set softseek off
   if (!EOF())
      do while (data >= Arg1 .AND. data <= Arg2)
         if (ncodigo != 0)
            if (fornecedor != ncodigo)
               skip 
               loop
            endif
         endif
         nforn:= fornecedor
         nnumero:= num_docto
         valfr:= data
         nvaltot:= valor_tot
         ddt_venc:= tipo_docto
         ndest:= destino
         select 3
         seek nforn
         if (Found())
            cnomefor:= nome
         else
            cnomefor:= " Fornecedor nao cadastrado "
         endif
         select 1
         if (!adiciona())
            loop
         endif
         replace codigo_for with nforn
         replace nome_for with cnomefor
         replace num_doc with nnumero
         replace data with valfr
         replace valor_tot with nvaltot
         replace destino with ndest
         replace tipo_doc with ddt_venc
         select 2
         skip 
         nlinha++
      enddo
   endif
   select 2
   close
   select 3
   close
   return

